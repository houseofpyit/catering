        <!-- some odoo version not support, please remove this if you are using odoo9 -->
        <template id="replace_login" name="Login Layout" inherit_id="web.login_layout">
            <xpath expr="//a[@target='_blank']" position="replace">
                <a href="https://www.amazesoftware.in/" target="_blank">Powered by <span>House of PY</span></a>
            </xpath>
        </template>

        <template id="web.layout" name="Web layout">&lt;!DOCTYPE html&gt;
        <html t-att="html_data or {}">
            <head>
                <meta charset="utf-8"/>
                <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>

                <title t-esc="title or 'AMAZE'"/>
                <link type="image/x-icon" rel="shortcut icon" t-att-href="x_icon or '/web/static/img/favicon.ico'"/>

                <script id="web.layout.odooscript" type="text/javascript">
                    var odoo = {
                        csrf_token: "<t t-esc="request.csrf_token(None)"/>",
                        debug: "<t t-esc="debug"/>",
                    };
                </script>

                <t t-out="head or ''"/>
            </head>
            <body t-att-class="body_classname">
                <t t-out="0"/>
            </body>
        </html>
    </template>
</odoo>