        <field name="print_report_name">
            '%s - %s - %s - %s' % (object.venue_address[:10],object.date,object.party_name_id.name,dict(object._fields['meal_type'].selection).get(dict(object._fields['meal_type'].selection).get(object.meal_type)))</field>
        <field name="binding_model_id" ref=""/>
        <field name="binding_type">report</field>
        </record>

