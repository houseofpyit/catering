        <field name="report_name">purchase.report_purchasequotation</field>
        <field name="report_file">purchase.report_purchasequotation</field>
        <field name="print_report_name">'Request for Quotation - %s' % (object.name)</field>
        <field name="binding_model_id" ref=""/>
        <field name="binding_type">report</field>
    </record>

    <record id="paper_format_form_invoice" model="report.paperformat">
        <field name="name">Invoices</field>
        <field name="format">A4</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">5</field>
        <field name="margin_bottom">20</field>
        <field name="margin_left">2</field>
        <field name="margin_right">2</field>
        <field name="dpi">90</field>
    </record>

    <record id="action_invoice_print_format" model="ir.actions.report">
        <field name="name">Invoices Print</field>
        <field name="model">account.move</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">ct_function_v15.invoice_prints</field>
        <field name="report_file">ct_function_v15.invoice_print</field>
        <field name="print_report_name">'Invoices - %s' % (object.name)</field>
        <field name="binding_model_id" ref="account.model_account_move"/>
        <field name="paperformat_id" ref="ct_function_v15.paper_format_form_invoice" />
        <field name="binding_type">report</field>
    </record>

    <record id="account.account_invoices_without_payment" model="ir.actions.report">
        <field name="name">Invoices without Payment</field>
        <field name="model">account.move</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">account.report_invoice</field>
        <field name="report_file">account.report_invoice</field>
        <field name="print_report_name">(object._get_report_base_filename())</field>
        <field name="attachment">(object.state == 'posted') and ((object.name or 'INV').replace('/','_')+'.pdf')</field>
        <field name="binding_model_id" ref=""/>
        <field name="binding_type">report</field>
    </record>

    <record id="account.account_invoices" model="ir.actions.report">
        <field name="name">Invoices</field>
        <field name="model">account.move</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">account.report_invoice_with_payments</field>
        <field name="report_file">account.report_invoice_with_payments</field>
        <field name="print_report_name">(object._get_report_base_filename())</field>
        <field name="attachment">(object.state == 'posted') and ((object.name or 'INV').replace('/','_')+'.pdf')</field>
        <field name="binding_model_id" ref=""/>
        <field name="binding_type">report</field>
        <field name="groups_id" eval="[(4, ref('account.group_account_invoice')),
(4, ref('account.group_account_readonly'))]"/>
    </record>

    <record id="paper_format_form_meeting" model="report.paperformat">
        <field name="name">Meeting</field>
        <field name="format">custom</field>
        <field name="page_height">120</field>
        <field name="page_width">150</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">2</field>
        <field name="margin_bottom">2</field>
        <field name="margin_left">2</field>
        <field name="margin_right">2</field>
        <field name="dpi">90</field>
    </record>

    <record id="action_meeting_print_format" model="ir.actions.report">
        <field name="name">Meeting Print</field>
        <field name="model">hop.meeting</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">ct_function_v15.meeting_prints</field>
        <field name="report_file">ct_function_v15.meeting_prints</field>
        <field name="print_report_name">'Meetings - %s' % (object.name)</field>
        <field name="binding_model_id" ref="ct_function_v15.model_hop_meeting"/>
        <field name="paperformat_id" ref="ct_function_v15.paper_format_form_meeting" />
        <field name="binding_type">report</field>
    </record>

</odoo>