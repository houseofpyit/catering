<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hop_cancel_report_tree_view_custom" model="ir.ui.view">
        <field name="name">hop.cancel.report.view.tree.custom</field>
        <field name="model">hop.cancel.report</field>
        <field name="arch" type="xml">
            <tree string="" export_xlsx="false" expand="1" create="0" edit="0" delete="0">
                <field name="date"/>
                <field name="meal_type" />
                <field name="cancel_type"/>
                <field name="no_of_pax"/>
                <field name="venue_address"/>
                <field name="reason"/>
            </tree>
        </field>
    </record>
    <record id="hop_cancel_report_action" model="ir.actions.act_window">
        <field name="name">Cancel Report</field>
        <field name="res_model">hop.cancel.report</field>
        <field name="view_mode">tree</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>
</odoo>