<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hop_product_running_report_treeview_custom" model="ir.ui.view">
        <field name="name">hop.product.running.report.view.tree</field>
        <field name="model">hop.product.running.report</field>
        <field name="arch" type="xml">
            <tree string="Runnig Stock" create="false" edit="false" delete="false">
                <field name="product_id" />
                <field name="date"/>
                <field name="open" />
                <field name="purchase"/>
                <field name="use"/>
                <field name="close"/>
            </tree>
        </field>
    </record>
    <record id="hop_product_running_report_action" model="ir.actions.act_window">
        <field name="name">Runnig Stock</field>
        <field name="res_model">hop.product.running.report</field>
        <field name="view_mode">tree</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>
</odoo>